<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
<!-- For seventh -->
    <bean id="employee" class="seventh.Employee" autowire="byName">
    <property name="name" value="Grey"/>
    </bean>
    
    <bean id="add" class="seventh.Address">
    <property name="hno" value="153"/>
    <property name="city" value="Gurgaon"/>
    </bean>
    
    <bean id="dep" class="seventh.Department">
    <property name="depName" value="Science"/>
    </bean>
</beans>
